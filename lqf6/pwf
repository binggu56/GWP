set terminal postscript eps enhanced color "Times-Roman" 24
#set size 0.5,0.5
set output 'wf.eps'
set xr [0.5:2.5]
set xlabel "x [a.u.]"
set ylabel "{/Symbol r}(x,t)"
plot 't100/wf.dat' u 1:2 w l lw 2 ti 't=100', \
     't300/wf.dat' u 1:2 w l lw 2 ti 't=300', \
     't500/wf.dat' u 1:2 w l lw 2 ti 't=500', \
     't600/wf.dat' u 1:2 w l lw 2 ti 't=600', \
     'wf0.dat' u 1:2 pt 1 ti "{/Symbol r}(x,0)"
